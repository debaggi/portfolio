    "logging.basicConfig(format='%(asctime)s - %(levelname)s - %(message)s', level=logging.INFO)\n",
    "\n",
    "def init_driver(default_proxy=None):\n",
    "    chr_options = Options()\n",
    "    chr_options.add_argument(\"--headless\")\n",
    "    chr_options.add_argument(\"--disable-blink-features=AutomationControlled\")\n",
    "\n",
    "    if default_proxy:\n",
    "        chr_options.add_argument(f'--proxy-server={default_proxy}')\n",
    "\n",
    "        try:\n",
    "            driver = webdriver.Chrome(options=chr_options)\n",
    "            logging.info(\"Driver successfully launched\")\n",
    "        except Exception as e:\n",
    "            logging.error(f\"Error: {e}\")\n",
    "            return None\n",
    "\n",
    "def scroll_page(driver):\n",
    "    # –ø–æ–ª—É—á–∞—é –≤—ã—Å–æ—Ç—É —Å—Ç—Ä–∞–Ω–∏—Ü—ã (–¥–ª–∏–Ω–∞ —Å—Ç—Ä–∞–Ω–∏—Ü—ã —Å–æ –≤—Å–µ–º–∏ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–∞–º–∏ –≤—ã–¥–∞—á–∏)\n",
    "    last_height = driver.execute_script(\"return document.body.scrollHeight\")\n",
    "    \n",
    "    while True:\n",
    "        # —Å–∫—Ä–æ–ª–ª–∏–Ω–≥ –≤–¥–æ–ª—å –¥–∏–∞–ø–∞–∑–æ–Ω–∞ –≤–∏–¥–∏–º–æ—Å—Ç–∏ —Å—Ç—Ä–∞–Ω–∏—Ü—ã\n",
    "        driver.execute_script(\"window.scrollTo(0, document.body.scrollHeight);\")\n",
    "        time.sleep(2)\n",
    "\n",
    "        # —Ñ–∏–∫—Å–∏—Ä—É—é –Ω–æ–≤—É—é –≤—ã—Å–æ—Ç—É –ø–æ—Å–ª–µ —Å–∫—Ä–æ–ª–ª–∏–Ω–≥–∞\n",
    "        new_height = driver.execute_script(\"return document.body.scrollHeight\")\n",
    "        \n",
    "        # –µ—Å–ª–∏ –Ω–æ–≤–∞—è –≤—ã—Å–æ—Ç–∞ == –ø—Ä–µ–¥—ã–¥—É—â–µ–π –≤—ã—Å–æ—Ç–µ --> —Å—Ç—Ä–∞–Ω–∏—Ü–∞ –ø—Ä–æ–π–¥–µ–Ω–∞ –Ω–∞ 100%\n",
    "        # –∏–Ω–∞—á–µ, –µ—Å–ª–∏ –Ω–æ–≤–∞—è –≤—ã—Å–æ—Ç–∞ != –ø—Ä–µ–¥—ã–¥—É—â–µ–π,\n",
    "        # —Ç–æ–≥–¥–∞ —Å–∫—Ä–æ–ª–ª–∏–Ω–≥ –ø—Ä–æ–¥–æ–ª–∂–∞–µ—Ç—Å—è\n",
    "        if new_height == last_height:\n",
    "            break\n",
    "   \n",
    "    \n",
    "    logging.info(\"Scrolling is done\")\n",
    "\n",
    "\n",
    "\n",
    "def wait_elements(driver):\n",
    "    try:\n",
    "        WebDriverWait(driver=driver, timeout=10).until(EC.presence_of_all_element_located(By.CLASS_NAME, \"vacancy-card\"))\n",
    "    except Exception as e:\n",
    "        logging.error(f\"–≤–∞–∫–∞–Ω—Å–∏–∏ –Ω–µ –Ω–∞–π–¥–µ–Ω—ã: {e}\")\n",
    "\n",
    "\n",
    "def get_vacancies(driver, URL):\n",
    "    all_vacancies_info = []\n",
    "    try:\n",
    "        # 1. –ø–µ—Ä–µ—Ö–æ–¥ –∫ —Ü–µ–ª–µ–≤–æ–º—É —Ä–µ—Å—É—Ä—Å—É\n",
    "        # 2. –æ–∂–∏–¥–∞–Ω–∏–µ –∑–∞–≥—Ä—É–∑–∫–∏ —Ö–æ—Ç—è –±—ã –æ–¥–Ω–æ–≥–æ –∏—Å–∫–æ–º–æ–≥–æ —ç–ª–µ–º–µ–Ω—Ç–∞\n",
    "        # 3. —Å–∫—Ä–æ–ª–ª–ª–∏–Ω–≥ —Å—Ç—Ä–∞–Ω–∏—Ü—ã —Å —Ä–µ–∑—É–ª—å—Ç–∞—Ç–∞–º–∏\n",
    "\n",
    "        driver.get(URL)\n",
    "        wait_elements(driver=driver)\n",
    "        scroll_page(driver=driver)\n",
    "\n",
    "        cards = driver.find_elements(By.CLASS_NAME, \"vacancy-card\")\n",
    "        logging.info(f\"Find vacancies cards: {len(cards)}\")\n",
    "\n",
    "        for card in cards:\n",
    "            try:\n",
    "                vacancy_info = {}\n",
    "                vacancy_info[\"title\"] = card.find_element(By.CLASS_NAME, \"vacancy-card__title\").text.strip()\n",
    "                vacancy_info[\"company_name\"] = card.find_element(By.CLASS_NAME, \"vacancy-card__company-title\").text.strip()\n",
    "                tags = [tag.text.strip() for tag in card.find_elements(By.CLASS_NAME, \"inline_list__item\")]\n",
    "\n",
    "                all_vacancies_info.append(vacancy_info)\n",
    "            except Exception as e:\n",
    "                logging.warning(msg=f\"card error loading: {e}\")\n",
    "    \n",
    "    except Exception as e:\n",
    "        logging.error(f\"page error loading: {e}\")\n",
    "    \n",
    "    return all_vacancies_info"
   ]
